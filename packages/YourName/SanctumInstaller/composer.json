{
  "name": "yourname/sanctum-installer",
  "description": "Auto-install sanctum and setup basic API auth for Laravel 11",
  "type": "library",
  "autoload": {
    "psr-4": {
      "YourName\\SanctumInstaller\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "laravel/sanctum": "^4.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourName\\SanctumInstaller\\SanctumInstallerServiceProvider"
      ]
    }
  }
}